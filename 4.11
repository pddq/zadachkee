/*Даны вещественные положительные числа a, b, c. Выяснить, существует ли
треугольник со сторонами a, b, c.*/

#include <iostream>
using namespace std;

int main() {

int a, b, c;

cin >> a;
cin >> b;
cin >> c;

if ((a+b)>c && (a+c)>b && (b+c)>a) {
  cout << "suschestvuet" << endl;
}
else { 
  cout << "ne suschestvuet" << endl;
}

}
