/*Одноклеточная амеба каждые три часа делится на 2 клетки. определить,
сколько клеток будет через 3, 6, 9, ..., 24 часа, если первоначально была
одна амеба.*/

#include <iostream>
#include <math.h>
using namespace std;

int main() {
int a;
a=1;
for(int i = 3; i<=24; i=i+3){
	a = a * 2;
	cout << "V "<< i << " ch " << a << " kl" << endl;
}
}

