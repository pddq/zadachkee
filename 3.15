/*Даны два целых числа а и в. Если а делится на в или в делится на а, то
вывести 1, иначе — любое другое число. Условные операторы и операторы
цикла не использовать.*/

#include <iostream>
using namespace std;

int main() {
  int a, b, c1, c2;
  a = 2;
  b = 2;
  c1 = a/b;
  c2 = b/a;
  cout<< c1*c2+1 <<endl;
}
