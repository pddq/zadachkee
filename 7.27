записи которых нет одинаковых цифр. Операции деления, целочисленного
деления и определения остатка не использовать.*/


#include <iostream>
using namespace std;

int main() {

  int n, j, k, i, s;
  n=100;
  j=0;
  k=0;
  i=0;
  while(n<=999) {
    while(k<10){
      while(j<10){

        if(k!=i+1 && j!=i+1 && k!=j){
         cout<<n<<endl;  
        }

        n++;
        j++;
      }
      j=0;
      k++;
    }
    k=0;
    i++;
  }

}
