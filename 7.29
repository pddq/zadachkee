/*Дано натуральное число N. Если в нем есть числа a и b, то определить,
какая из них расположена в числе правее. Если одна или обе эти цифры
встречаются в числе несколько раз, то должны быть рассмотрены самые
правые из одинаковых цифр.*/

#include <iostream>
using namespace std;

int main() {


int n, a, b, i, m;
n=32512;
a=1;
b=2;


while(n>0){

m=n%10;
if(m==a){
cout<<m<<" pravee"<<endl;
break;
}
if(m==b){
cout<<m<<" pravee"<<endl;
break;
}

n=n/10;
}

}
