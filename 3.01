#include <iostream>
using namespace std;
 
int main(){
  float x, y;
  cout << "Vvedite znachenie x" <<endl;
  cin >> x;
  cout << "Vvedite znachenie y" <<endl;
  cin >> y;

  float z = x/y;
  float c = x*y;
  float b = x-y;

  cout << "x/y: "<< z<< "\n";
  cout << "x*y: "<< c<< "\n";
  cout << "x-y: "<< b<< "\n";
}
